// setup function

void setup() {
  size(600, 400); // sets the canvas size 
  background(178, 255, 236); // set background color
  
  fill(252, 8, 122); // color of the square 
  strokeWeight(5); // width of border 
  stroke(255, 0, 0); // border color
  rect( 300, 200, 100, 100); // draw a square 
  
  // triangle
  fill(252, 245, 0);
  triangle(300, 200, 350, 125, 400, 200);
  
} // end setup

